@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://workflow.org/> .

:Database a rdfs:Class .
:Process a rdfs:Class .
:Step a rdfs:Class .
:ConstructStep rdfs:subClassOf :Step .
:AskStep rdfs:subClassOf :Step .
:DatabaseUpdateStep rdfs:subClassOf :Step .
:FinalStep rdfs:subClassOf :Step .

:startAt a rdf:Property ;
    rdfs:domain :Process ;
    rdfs:range :Step .

:next a rdf:Property ;
    rdfs:domain :Step ;
    rdfs:range :Step .

:nextOnTrue a rdf:Property ;
    rdfs:domain :AskStep ;
    rdfs:range :Step .

:nextOnFalse a rdf:Property ;
    rdfs:domain :AskStep ;
    rdfs:range :Step .

:sparql a rdf:Property ;
    rdfs:domain :Step ;
    rdfs:range xsd:string .

:success a rdf:Property ;
    rdfs:domain :FinalStep ;
    rdfs:range xsd:boolean .
